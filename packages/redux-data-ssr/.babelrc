{
  "presets": [
    [
      "env",
      {
        "modules": false
      }
    ],
    "react"
  ],
  "plugins": [
    [
      "babel-plugin-transform-object-rest-spread",
      { "useBuiltIns": true }
    ]
  ],
  "env": {
    "test": {
      "presets": [
        "env",
        "react"
      ],
      "plugins": [
        [
          "transform-es2015-modules-commonjs",
          { "loose": true }
        ]
      ]
    }
  }
}
